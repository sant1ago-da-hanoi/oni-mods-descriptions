msgstr "<link=\"SANDCEMENT\">Xi măng</link>"

#. STRINGS.ELEMENTS.SANDSTONE.DESC
msgctxt "STRINGS.ELEMENTS.SANDSTONE.DESC"
msgid "Sandstone is a composite of relatively soft sedimentary rock.\n\n"
"It is useful as a <b>Construction Material</b>."
msgstr "Sa thạch là một hỗn hợp đá trầm tích tương đối mềm.\n\n"
"Sa thạch được sử dụng như một</b>Vật liệu xây dựng</b>."

#. STRINGS.ELEMENTS.SANDSTONE.NAME
msgctxt "STRINGS.ELEMENTS.SANDSTONE.NAME"
msgid "<link=\"SANDSTONE\">Sandstone</link>"
msgstr "<link=\"SANDSTONE\">Đá sa thạch</link>"

#. STRINGS.ELEMENTS.SEDIMENTARYROCK.DESC
msgctxt "STRINGS.ELEMENTS.SEDIMENTARYROCK.DESC"
msgid "Sedimentary Rock is a hardened composite of sediment layers.\n\n"
"It is useful as a <b>Construction Material</b>."
msgstr "Đá trầm tích là một hỗn hợp đá tương đối cứng.\n\n"
"Đá trầm tích được sử dụng như một</b>Vật liệu xây dựng</b>."

