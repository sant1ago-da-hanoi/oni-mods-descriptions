#. STRINGS.BUILDINGS.PREFABS.CARGOBAY.DESC
#| msgctxt "STRINGS.BUILDINGS.PREFABS.SOLIDCARGOBAYSMALL.DESC"
#| msgid "Duplicants will fill cargo bays with any resources they find during space missions."
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAY.DESC"
msgid "Duplicants will fill cargo bays with any resources they find during space missions."
msgstr "Thằng Đệ sẽ cất đầy các khoang hàng hóa bằng bất kỳ tài nguyên nào mà chúng nó tìm thấy trong các nhiệm vụ không gian."

#. STRINGS.BUILDINGS.PREFABS.CARGOBAY.EFFECT
#| msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAY.EFFECT"
#| msgid ""
#| "Allows Duplicants to store most of the <link=\"ELEMENTSSOLID\">Solid Materials</link> found during space missions.\n"
#| "\n"
#| "Stored resources become available to the colony upon the rocket's return."
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAY.EFFECT"
msgid "Allows Duplicants to store any <link=\"ELEMENTSSOLID\">Solid Materials</link> found during space missions.\n\n"
"Stored resources become available to the colony upon the rocket's return."
msgstr "Cho phép đệ cất trữ tất cả các loại <link=\"ELEMENTSSOLID\">Vật liệu Rắn</link> mà nó tìm thấy trong các nhiệm vụ không gian.\n\n"
"Tài nguyên sẽ sẵn sàng để sử dụng khi mang về nhà."

#. STRINGS.BUILDINGS.PREFABS.CARGOBAY.NAME
#| msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAY.NAME"
#| msgid "<link=\"CARGOBAY\">Large Cargo Bay</link>"
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAY.NAME"
msgid "<link=\"CARGOBAY\">Cargo Bay</link>"
msgstr "<link=\"CARGOBAY\">Thùng Hàng Hóa</link>"

#. STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.DESC"
msgid "Holds more than a regular cargo bay."
msgstr "Chứa nhiều hơn một khoang hàng hóa thông thường."

#. STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.EFFECT
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.EFFECT"
msgid "Allows Duplicants to store most of the <link=\"ELEMENTSSOLID\">Solid Materials</link> found during space missions.\n\n"
"Stored resources become available to the colony upon the rocket's return."
msgstr "Cho phép thằng đệ lưu trữ hầu hết các <link=\"ELEMENTSSOLID\">Vật liệu Rắn</link> được tìm thấy trong các nhiệm vụ không gian.\n\n"
"Tài nguyên được lưu trữ sẽ có sẵn cho thuộc địa khi tên lửa trở về."

#. STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CARGOBAYCLUSTER.NAME"
msgid "<link=\"CARGOBAY\">Large Cargo Bay</link>"
msgstr "<link=\"CARGOBAY\">Thùng Hàng Lớn</link>"

#. STRINGS.BUILDINGS.PREFABS.CARPETTILE.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CARPETTILE.DESC"
msgid "Soft on little Duplicant toesies."
msgstr "Mềm mại trên những ngón chân nhỏ bé của Thằng Đệ."

#. STRINGS.BUILDINGS.PREFABS.CARPETTILE.EFFECT
msgctxt "STRINGS.BUILDINGS.PREFABS.CARPETTILE.EFFECT"
msgid "Used to build the walls and floors of rooms.\n\n"
"Increases <link=\"DECOR\">Decor</link>, contributing to <link=\"MORALE\">Morale</link>."
msgstr "Được sử dụng để xây tường và sàn nhà.\n\n"
"Tăng <link=\"DECOR\">Trang trí</link>, đóng góp vào điểm <link=\"MORALE\">Tinh Thần</link>."

#. STRINGS.BUILDINGS.PREFABS.CARPETTILE.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CARPETTILE.NAME"
msgid "<link=\"CARPETTILE\">Carpeted Tile</link>"
msgstr "<link=\"CARPETTILE\">Gạch trải thảm</link>"

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.DESC"
msgid "Light reduces Duplicant stress and is required to grow certain plants."
msgstr "Ánh sáng làm giảm stress và là một điều kiện cần thiết cho quá trình phát triển của một số loài cây."

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.EFFECT
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.EFFECT"
msgid "Provides <link=\"LIGHT\">Light</link> when <link=\"POWER\">Powered</link>.\n\n"
"Increases Duplicant workspeed within light radius."
msgstr "Cung cấp <link=\"LIGHT\">Ánh sáng</link> khi <link=\"POWER\">Được cấp điện</link>.\n\n"
"Tăng tốc độ làm việc thằng đệ trong bán kính ánh sáng."

#. STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CEILINGLIGHT.NAME"
msgid "<link=\"CEILINGLIGHT\">Ceiling Light</link>"
msgstr "<link=\"CEILINGLIGHT\">Đèn trần</link>"

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.DESC"
msgid "Checkpoints can be connected to automated sensors to determine when it's safe to enter."
msgstr "Điểm dừng có thể được nối với các Cảm biến tự động để ra quyết định khi nào nơi đó an toàn để đi vào."

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.EFFECT
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.EFFECT"
msgid "Allows Duplicants to pass when receiving a <b><style=\"logic_on\">Green Signal</style></b>.\n\n"
"Prevents Duplicants from passing when receiving a <b><style=\"logic_off\">Red Signal</style></b>."
msgstr "Cho phép các Thằng Đệ đi qua khi nhận được <b><b><style=\"logic_on\">Tín Hiệu Xanh</style></b>.\n\n"
"Ngăn các Thằng Đệ không được đi qua khi nhận được <b><style=\"logic_off\">Tín hiệu Đỏ</style></b>."

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT"
msgid "Duplicant Stop/Go"
msgstr "Đệ Dừng/Di chuyển"

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT_ACTIVE
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT_ACTIVE"
msgid "<b><style=\"logic_on\">Green Signal</style></b>: Allow Duplicant passage"
msgstr "<b><style=\"logic_on\">Tín Hiệu Xanh</style></b>: Cho phép bọn đệ đi qua"

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT_INACTIVE
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.LOGIC_PORT_INACTIVE"
msgid "<b><style=\"logic_off\">Red Signal</style></b>: Prevent Duplicant passage"
msgstr "<b><style=\"logic_off\">Tín Hiệu Đỏ</style></b>: Ngăn bọn đệ đi qua"

#. STRINGS.BUILDINGS.PREFABS.CHECKPOINT.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CHECKPOINT.NAME"
msgid "<link=\"CHECKPOINT\">Duplicant Checkpoint</link>"
msgstr "<link=\"CHECKPOINT\">Cửa điều kiện</link>"

#. STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.DESC
msgctxt "STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.DESC"
msgid "A textile loom can be used to spin Reed Fiber into wearable Duplicant clothing."
msgstr "Máy dệt có thể dùng để đan những sợi Reed thành những bộ đồ cho Đệ."

#. STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.EFFECT
msgctxt "STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.EFFECT"
msgid "Tailors Duplicant <link=\"EQUIPMENT\">Clothing</link> items.\n\n"
"Duplicants will not fabricate items unless recipes are queued."
msgstr "Gia công và may <link=\"EQUIPMENT\">Đồ</link>.\n\n"
"Đệ sẽ không chế tạo nếu không có nhiệm vụ được đặt trong hàng chờ."

#. STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.NAME
msgctxt "STRINGS.BUILDINGS.PREFABS.CLOTHINGFABRICATOR.NAME"
msgid "<link=\"CLOTHINGFABRICATOR\">Textile Loom</link>"
msgstr "<link=\"CLOTHINGFABRICATOR\">Máy dệt</link>"

